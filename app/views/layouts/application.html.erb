<!DOCTYPE html>
<html>
<head>
  <title>PracticeBlog</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%#= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="main" class="cf">

	<div id="header">
		<h1>Practice Blogger App</h1>
	</div>

	<section id="sidebar">
		<ul>
			<% @subjects.each do |subject| %>
			<li>
				<%= link_to subject.name, subject_path(subject) %>
			</li>
			<% end %>
		</ul>
	</section>

	<section id="content">
		<%= yield %>
	</section>

</div> <%# end main %>
</body>
</html>
