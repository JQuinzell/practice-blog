<table class="table">
  <thead>
    <th>Username</th>
    <th>Email</th>
    <th>Actions</th>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td id="<%= user.username.parameterize %>">
        <%= link_to "Edit", [:edit, :admin, user] %>
      </td>
    </tr>
  <% end %>
</table>